{
    "name": "@prismaui/core",
    "version": "0.0.1-y.10.3",
    "description": "Stencil Component Starter",
    "main": "dist/index.cjs.js",
    "module": "dist/index.js",
    "es2015": "dist/esm/index.mjs",
    "es2017": "dist/esm/index.mjs",
    "types": "dist/types/index.d.ts",
    "collection": "dist/collection/collection-manifest.json",
    "collection:main": "dist/collection/index.js",
    "unpkg": "dist/core/core.esm.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/prismaui/prismaui.git"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "files": [
        "dist/",
        "loader/"
    ],
    "scripts": {
        "build": "stencil build --docs",
        "start": "stencil build --dev --watch --serve",
        "test": "stencil test --spec --e2e",
        "test.watch": "stencil test --spec --e2e --watchAll",
        "generate": "stencil generate",
        "story": "storybook dev -p 6006",
        "build:story": "storybook build -o dist-storybook",
        "build-storybook": "storybook build"
    },
    "dependencies": {
        "@stencil/core": "^3.2.1"
    },
    "devDependencies": {
        "@stencil/angular-output-target": "^0.7.0",
        "@stencil/react-output-target": "^0.5.1",
        "@stencil/sass": "^3.0.2",
        "@stencil/vue-output-target": "^0.8.5",
        "@storybook/addon-essentials": "^7.0.12",
        "@storybook/addon-interactions": "^7.0.12",
        "@storybook/addon-links": "^7.0.12",
        "@storybook/addon-styling": "^1.0.8",
        "@storybook/blocks": "^7.0.12",
        "@storybook/html": "^7.0.12",
        "@storybook/html-vite": "^7.0.12",
        "@storybook/testing-library": "^0.0.14-next.2",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.18.11",
        "jest": "^27.5.1",
        "jest-cli": "^27.5.1",
        "puppeteer": "^19.5.2",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "sass": "^1.62.1",
        "storybook": "^7.0.12"
    },
    "license": "MIT"
}
